{
    "uploadImageAPI": {
        "url": "https://gtbzb9nn65.execute-api.us-west-2.amazonaws.com/v3",
        "paths": {
            "/":
            {
                "put": {
                    "summary": "store image in s3 bucket",
                    "parameter": {
                        "folder": {
                            "in": "path",
                            "description": "s3 bucket name that store images",
                            "required": "True"
                        },
                        "object": {
                            "in": "path",
                            "description": "image stored inside bucket (ex: test1.jpeg)",
                            "required": "True"
                        }
                    },
                    "responses": {
                        "200": {
                            "description": "object successfully stored inside bucket"
                        }
                    }
                }
            }
        }
    },
    "getImageDetails": {
        "url": "https://y09n0ijh1g.execute-api.us-west-2.amazonaws.com/v1",
        "paths": {
            "/":
            {
                "post": {
                    "summary": "retrieve image recognition details from DynamoDb table by image id (image filename)",
                    "parameter": {
                        "imageid": {
                            "in": "path",
                            "description": "image id stored in DynamoDb table",
                            "required": "True"
                        }
                    },
                    "responses": {
                        "200": {
                            "description": "image recognition details",
                            "content": "Application/json"
                        }
                    }
                }
            }
        }
    }

}